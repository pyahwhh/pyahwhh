    loadstring(game:HttpGet("https://pastebin.com/raw/jHBfJYmS"))()
end)

Player:Toggle("夜视", false, function(Value)
		if Value then
		    game.Lighting.Ambient = Color3.new(1, 1, 1)
		else
		    game.Lighting.Ambient = Color3.new(0, 0, 0)
		end
end)

